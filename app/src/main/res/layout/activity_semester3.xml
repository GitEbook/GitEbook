<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_semester3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/images"
    tools:context="com.example.saitarun.git.SEMESTER3">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </ScrollView>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <Button
            android:text="Engineering Mathematics-III (Elective)"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button10"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textColor="@color/colorPrimaryDark"/>

        <Button
            android:text="Object Oriented Programming with C++"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button24"
            android:textColor="@color/colorPrimaryDark"
            android:onClick="btcpp"/>

        <Button
            android:text="Computer Organization and Architecture"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button25"
            android:onClick="btcoa"
            android:textColor="@color/colorPrimaryDark"
            />

        <Button
            android:text="Data Communications"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button26"
            android:onClick="btdc"
            android:textColor="@color/colorPrimaryDark"/>

        <Button
            android:text="Programming Language Pragmatics"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button27"
            android:onClick="btplp"
            android:textColor="@color/colorPrimaryDark"/>

        <Button
            android:text="C++ Laboratory"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button28"
            android:textColor="@color/colorPrimaryDark"/>

        <Button
            android:text="UNIX Programming Laboratory"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button62"
            android:textColor="@color/colorPrimaryDark"/>

        <Button
            android:text="Computer Engineering Workshop"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button63"
            android:textColor="@color/colorPrimaryDark"/>
    </LinearLayout>

</RelativeLayout>
